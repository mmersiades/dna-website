{
  "name": "dna-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "sanity:predev": "pnpm run typegen",
    "sanity:prebuild": "pnpm run typegen",
    "sanity:typegen": "sanity schema extract --enforce-required-fields --path=./src/sanity/extract.json && sanity typegen generate",
    "prettier:write": "npx prettier . --write",
    "prettier:check": "npx prettier . --check",
    "prepare": "husky"
  },
  "dependencies": {
    "@sanity/client": "^7.14.1",
    "@sanity/image-url": "^1.2.0",
    "@sanity/vision": "^4.22.0",
    "clsx": "^2.1.1",
    "next": "16.1.4",
    "next-sanity": "^11.6.12",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sanity": "^4.22.0",
    "styled-components": "^6.3.8",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,json}": "prettier --write"
  }
}
